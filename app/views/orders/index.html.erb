<p id="notice"><%= notice %></p>

<h1>已完成订单</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td>订单<%= order.id %></td>
        <td><%= link_to '查看', order %></td>
        <td><%= link_to '删除', order, method: :delete , data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
